{
  "name": "reelms",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "dotenv -e .env -- next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test:db": "dotenv -e .env -- tsx scripts/test-db.ts",
    "db:migrate": "dotenv -e .env -- prisma migrate dev",
    "db:reset": "dotenv -e .env -- prisma migrate reset",
    "db:studio": "dotenv -e .env -- prisma studio",
    "db:seed": "dotenv -e .env -- prisma db seed",
    "db:generate": "dotenv -e .env -- prisma generate",
    "db:push": "dotenv -e .env -- prisma db push"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "dependencies": {
    "@prisma/client": "^6.19.0",
    "@react-three/drei": "^10.7.6",
    "@react-three/fiber": "^9.4.0",
    "@react-three/postprocessing": "^3.0.4",
    "@tailwindcss/vite": "^4.1.18",
    "iron-session": "^8.0.4",
    "jose": "^6.1.0",
    "next": "^16.0.7",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "stripe": "^20.0.0",
    "three": "^0.181.0",
    "zod": "^4.1.13",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@netlify/plugin-nextjs": "^5.15.7",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/three": "^0.181.0",
    "dotenv-cli": "^11.0.0",
    "eslint": "^9",
    "eslint-config-next": "16.0.1",
    "prisma": "^6.19.0",
    "tailwindcss": "^4.1.18",
    "tsx": "^4.20.6",
    "typescript": "^5"
  }
}
